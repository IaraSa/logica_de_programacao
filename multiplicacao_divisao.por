programa {
  funcao inicio() {
    real a, b, mult, div

    escreva("Por favor, informe o valor de A: ")
    leia(a)

    escreva("Por favor, informe o valor de B: ")
    leia(b)

    mult = a * b // multiplicar
    div = a / b // dividir

    escreva("O resultado da Multiplicação A e B é: ", mult, "\n")
    escreva("O resultado da Divisão A e B é: ", div, "\n")

  }
}
