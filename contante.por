// Entendendo sobre declaração de constantes
/* Constante é um identificador dentro de um valor que não pode ser alterados durante a execução do programa. */

programa {
  funcao inicio() {

    //Numerico (Interiro ou Real)
    inteiro numero = 12
    real valor = 12.5

    //Literal (Textos)
    caracter status = 'S'
    cadeia nome = 'Maria dos Remedios'

    //Logico
    logico compra = 'F'

    const inteiro LOJA_PRINCIPAL = 1234
    
  }
}
